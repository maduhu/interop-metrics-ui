{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "max-len": [2, {"code": 120}],
    "react/no-multi-comp": [0],
    "react/prop-types": 0,
    "react/prefer-stateless-function": [1, {"ignorePureComponents": true}],
    "react/jsx-filename-extension": 0,
    "import/no-named-as-default": 0,
    "no-trailing-spaces": 0
  }
}
